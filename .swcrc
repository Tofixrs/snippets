{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"dynamicImport": true,
			"decorators": true
		},
		"target": "es2022",
		"loose": true,
		"externalHelpers": true
	},
	"module": {
		"type": "commonjs",
		"strict": true,
		"strictMode": true,
		"lazy": true,
		"noInterop": true
	},
	"minify": false
}
